{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ],
  "env": {
    "APP_DEBUG": {
      "required": true
    },
    "APP_KEY": {
      "required": true
    },
    "APP_URL": {
      "required": true
    },
    "AWS_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "DB_CONNECTION": {
      "required": true
    },
    "S3_BUCKET": {
      "required": true
    },
    "S3_BUCKET_NAME": {
      "required": true
    },
    "S3_key": {
      "required": true
    },
    "S3_secret": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "sakahat",
  "scripts": {
  },
  "stack": "heroku-18"
}
